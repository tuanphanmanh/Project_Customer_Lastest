{
  "name": "abp-zero-template",
  "version": "12.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "publish": "set NODE_OPTIONS=--max_old_space_size=16384 && gulp build  && ng build --configuration production            ",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "post-publish": "shx cp -r ./wwwroot/dist/. ./wwwroot/",
    "publish-k8s": "gulp build && ng build --configuration k8s --verbose",
    "start": "set NODE_OPTIONS=--max_old_space_size=16384 && ng serve --host 0.0.0.0 --port 4200 ",
    "hmr": "gulp buildDev && ng serve --host 0.0.0.0 --port 4200 --hmr",
    "test": "gulp buildDev && ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "gulp buildDev && protractor",
    "create-dynamic-bundles": "gulp buildDev",
    "nswag": "cd nswag/ && refresh.bat",
    "deploy_dev": "ng build --env=dev --aot=true --output-hashing=all --extract-css=true && npm run add_date",
    "deploy_prd": "ng build --prod && npm run add_date",
    "add_date": "npm run add_date_js && npm run add_date_css",
    "add_date_js": "for i in dist/*; do if [ -f $i ]; then LC_ALL=C sed -i.bak 's:js\":js?'$(date +%H%M%m%d%y)'\":g' $i; fi done",
    "add_date_css": "sed -i.bak 's:css\":css?'$(date +%H%M%m%d%y)'\":g' dist/index.html",
    "rm_bak_files": "find dist -name '*.bak' -exec rm -Rf {} \\;"
  },
  "private": false,
